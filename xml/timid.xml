  </snc>
  <trdgrp lng="cs">
    <trd>bojácný</trd>,
    <trd>trpící trémou</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>pusillanime</trd>,
    <trd>timor&eacute;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bojazlivý</trd>,
    <trd>plachý</trd>,
    <trd>ľakavý</trd>
  </trdgrp>
</drv>
</art>

