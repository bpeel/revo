  <rad>timid</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="timid.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="timid.0a.PSI">
  <ref tip="dif" cel="hont.0ema.PSI">hontema</ref>
