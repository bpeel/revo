<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kilo.xml,v 1.2 2012/07/15 10:10:40 revo Exp $">
<kap>
  <rad>kilo</rad>- <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="kilo.0">
  <kap><tld/>-</kap>
  <mlg>k</mlg>
  <gra><vspec>prefikso</vspec></gra>
  <snc mrk="kilo.0.MEZ">
    <dif>
      &leftquot;Milo da&rightquot; (1000)
      en la Internacia Sistemo de unuoj (SI):
      <ekz>
        <ref tip="vid" cel="kilome.0o"><tld/>metro</ref>;
      </ekz>
      <ekz>
        <ref tip="vid" cel="kilovo.0o"><tld/>volto</ref>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>quilo-</trd>,
    <trd>kilo-</trd>
  </trdgrp>
  <trd lng="fr">kilo-</trd>
</drv>
</art>

