<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: alkemi.xml,v 1.3 2017/12/22 17:10:19 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>alkemi</rad>/o 
</kap>

<drv mrk="alkemi.0o">
  <kap><tld/>o <fnt><lok>Akademio de Esperanto, <url
  ref="http://www.akademio-de-esperanto.org/aktoj/aktoj2/8oa.html">8a
  Oficiala Aldono</url>, II.A.1</lok></fnt></kap>
  <snc>
    <ref tip="dif" cel="alhxem.0o">al&hcirc;emio</ref>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_mol;&c_h;&c_ib;&c_m;&c_ib;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Alchemie</trd>,
    <trd>Alchimie</trd>,
    <trd>Goldmacherkunst</trd>
  </trdgrp>
  <trd lng="en">alchemy</trd>
  <trd lng="es">alquimia</trd>
  <trd lng="fr">alchimie</trd>
  <trdgrp lng="nl">
    <trd>alchemie</trd>,
    <trd>alchimie</trd>
  </trdgrp>
  <trd lng="pl">alchemia</trd>
  <trd lng="pt">alquimia</trd>
  <trd lng="ru">&c_a;&c_l;&c_h;&c_i;&c_m;&c_i;&c_ja;</trd>
  <trd lng="sk">alch&yacute;mia</trd>
</drv>

<drv mrk="alkemi.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <ref tip="dif" cel="alhxem.0isto">al&hcirc;emiisto</ref>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_mol;&c_h;&c_ib;&c_m;&c_ib;&c_k;</trd>
  <trd lng="de">Alchimist</trd>
  <trd lng="en">alchemist</trd>
  <trd lng="es">alquimista</trd>
  <trd lng="fr">alchimiste</trd>
  <trdgrp lng="nl">
    <trd>alchemist</trd>,
    <trd>alchimist</trd>
  </trdgrp>
  <trd lng="pl">alchemik</trd>
  <trd lng="pt">alquimista</trd>
  <trd lng="ru">&c_a;&c_l;&c_h;&c_i;&c_m;&c_i;&c_k;</trd>
  <trd lng="sk">alchymista</trd>
</drv>
</art>

