<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: klon1.xml,v 1.9 2019/02/18 23:10:30 revo Exp $">
<kap>
  <rad>klon</rad>/o
</kap>
<drv mrk="klon1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <ref tip="dif" cel="klonik.0aspasmo">klonika spasmo</ref>
  </snc>
  <rim>
    <ref tip="hom" cel="klon.0o">Klono</ref>
    estas anka&ubreve; kopio de viva&jcirc;o a&ubreve; komputila programo.
  </rim>
  <adm>
     PIV2 nomas tion "klonuso".
     <aut>MB</aut>
  </adm>
  <trd lng="ca">clon <klr>(reflex)</klr></trd>
  <trdgrp lng="cs">
    <trd>klonus</trd>,
    <trd>&scaron;kubav&aacute; k&rcaron;e&ccaron;</trd>
  </trdgrp>
  <trd lng="de">Klonus</trd>
  <trd lng="nl">klonisch <ind>spasma</ind></trd>
  <trd lng="pl">konwulsja</trd>     
  <trd lng="ro">convulsie</trd>
  <trdgrp lng="sk">
    <trd>klonus</trd>,
    <trd>&scaron;klbav&yacute; k&racute;&ccaron;</trd>
  </trdgrp>
</drv>

<drv mrk="klon1.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">MED</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <ref tip="dif" cel="klonik.0a">klonika</ref>
  </snc>
  <trd lng="ca">cl&ograve;nic</trd>
  <trd lng="de">klonisch</trd>
  <trd lng="nl">klonisch</trd>
  <trd lng="pl">klonowy</trd>     
  <trd lng="ro">ar&#539;ar</trd>
</drv>
</art>

