<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fet.xml,v 1.16 2011/10/08 20:10:11 revo Exp $">
<kap>
<rad>fet</rad>/o
</kap>
<drv mrk="fet.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Nomo de l' embrio ekde la tempo, kiam &gcirc;i vidigas trajtojn de
      esta&jcirc;o
      kaj &gcirc;is la tempo, kiam &gcirc;i forlasas la uteron de la patrino. 
    </dif>
    <refgrp tip="vid">
      <ref cel="embri.0o">embrio</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_o;&c_d; <klr tip="amb">(&c_v;&c_a; &c_w;&c_l;&c_o;&c_n;&c_mol;&c_n;&c_ib;)</klr></trd>
    <trdgrp lng="de">
     <trd>F&ouml;tus</trd>,
     <trd>Fetus [science]</trd>,
     <trd>Leibesfrucht</trd></trdgrp>
  <trd lng="en">fetus</trd>
  <trd lng="es">feto</trd>
  <trd lng="fa">&a_j;&a_n;&f_y;&a_n;</trd>
  <trd lng="fr">f&oelig;tus</trd>
  <trdgrp lng="hu">
    <trd>fetus</trd>,
    <trd>fejl&odblac;d&odblac; <ind>magzat</ind></trd>
  </trdgrp>
  <trd lng="it">feto</trd>
  <trd lng="nl">foetus</trd>
  <trd lng="pl">p&lstroke;&oacute;d</trd>
  <trd lng="pt">feto</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_t;&c_r;&c_o;&c_b;&c_n;&c_y;&c_j; &c_p;&c_l;&c_o;&c_d;</trd>
  </trdgrp>
</drv>
</art>
