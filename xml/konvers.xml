<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: konvers.xml,v 1.1 2017/03/28 16:10:19 revo Exp $">
<kap>
  <rad>konvers</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="konvers.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="konvers.0i.KOMUNE">
    <ref tip="dif" cel="konver2.0i.KOMUNE">konversacii</ref>
    <ekz>
      lastatempe, kiam li <tld/>is kun mi, lia parolo &ccirc;iam tu&scirc;is Qin
      <fnt><bib>LaFamilio</bib>, <lok>&ccirc;apitro 11a, p. 122a</lok></fnt>.
    </ekz>
  </snc>
  <rim mrk="konvers.0i.RIM_HOM">
    &leftquot;<ref tip="hom" cel="konver1.0i">Konversi</ref>&rightquot; aperas anka&ubreve; kiel
    ekonomia termino.
  </rim>
  <trd lng="fr">s'<ind>entretenir</ind></trd>
</drv>
</art>

