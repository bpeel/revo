<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: polistirol.xml,v 1.2 2015/03/25 09:10:16 revo Exp $">
<kap>
    <rad>polistirol</rad>/o
</kap>
<drv mrk="polistirol.0o">
  <kap><tld/>o</kap>
  <snc mrk="polistirol.0o.polistireno">
    <uzo tip="fak">KEM</uzo>
      <ref tip="dif" cel="polistiren.0o">polistireno</ref>      
      <ekz>
        la hodia&ubreve;aj laboratoriaj iloj estas produktitaj plejofte el
         polipropileno kaj <tld/>o
        <fnt><bib>Viki</bib>, <lok>In vitro</lok></fnt>;
      </ekz>
      <ekz>
         celofano, <tld/>o kaj polikarbonato estas duoblorefraktaj
         materialoj
         <fnt>
           <bib>Viki</bib>,
           <lok>Duobla refrakto</lok>
         </fnt>.
      </ekz>
  </snc>
  <trd lng="fr">polystyrol</trd>
</drv>
</art>

