    401 a.K.
    </dif>
  </snc>
  <trd lng="ca">An&agrave;basi</trd>
  <trd lng="el">
  &Alfa_psili;&ny;&alfa_ton;&beta;&alfa;&sigma;&jota;&sigma_fina;</trd>
  <trd lng="it">Anabasi</trd>
  <trdgrp lng="pl">
    <trd>Anabaza</trd>,
    <trd>Wyprawa Cyrusa</trd>
  </trdgrp>
</drv>
</art>

