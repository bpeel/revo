<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: arund.xml,v 1.1 2013/10/27 19:10:19 revo Exp $">
<kap>
    <rad>arund</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="arund.0o">
  <kap><tld/>o</kap>
  <snc mrk="arund.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="kan.0o.BOT" lst="voko:pooparencoj">Kano</ref>,
      kreskanta en Mediteraneo kaj Tajvano;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">genro</ref>
      el familio <ref tip="malprt" cel="poac.0oj">poacoj</ref>
      (<trd lng="la">Arundo</trd>).
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Pfahlrohr</trd>,
    <trd>Spanisches Rohr</trd>
  </trdgrp>
</drv>
</art>

